[
  // https://stackoverflow.com/questions/39599514/vs-code-add-a-new-file-under-the-selected-working-directory
  {
    "key": "ctrl+n",
    "command": "explorer.newFile",
    "when": "explorerViewletFocus"
  },
  {
    "key": "ctrl+shift+n",
    "command": "explorer.newFolder",
    "when": "explorerViewletFocus"
  }
]
